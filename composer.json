{
    "name": "northcreation-agency/silverstripe-headless-rest",
    "description": "Headless rest api for Silverstripe CMS and various front end frameworks",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS",
        "Headless",
        "Rest",
        "Api",
        "NextJS",
        "NuxtJS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "OliverNorden\\HeadlessRest\\": "src/",
            "OliverNorden\\HeadlessRest\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
